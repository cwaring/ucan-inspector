{
  "name": "@ucan-wg/inspector",
  "type": "module",
  "version": "0.2.3",
  "packageManager": "pnpm@10.30.2",
  "description": "UCAN Inspector, a webcomponent for inspecting UCAN tokens and containers",
  "author": "Chris Waring (https://github.com/cwaring)",
  "license": "MIT OR Apache-2.0",
  "homepage": "https://github.com/cwaring/ucan-inspector#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cwaring/ucan-inspector.git"
  },
  "bugs": {
    "url": "https://github.com/cwaring/ucan-inspector/issues"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.mts",
  "files": [
    "LICENSE-APACHE",
    "LICENSE-MIT",
    "dist"
  ],
  "scripts": {
    "build:css": "tsx scripts/build-css.ts",
    "build": "pnpm build:css && tsdown",
    "dev": "pnpm build:css && tsdown --watch",
    "lint": "eslint",
    "play": "nr -C playground dev",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "vue": "catalog:prod"
  },
  "dependencies": {
    "@ipld/dag-json": "catalog:prod",
    "cborg": "catalog:prod",
    "date-fns": "catalog:prod",
    "iso-base": "catalog:prod",
    "iso-did": "catalog:prod",
    "iso-signatures": "catalog:prod",
    "iso-ucan": "catalog:prod",
    "multiformats": "catalog:prod",
    "pako": "catalog:prod"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:dev",
    "@antfu/ni": "catalog:dev",
    "@types/node": "catalog:types",
    "@types/pako": "catalog:types",
    "@unocss/reset": "catalog:dev",
    "ansis": "catalog:dev",
    "bumpp": "catalog:dev",
    "chokidar": "catalog:dev",
    "eslint": "catalog:dev",
    "exsolve": "catalog:dev",
    "lightningcss": "catalog:dev",
    "lint-staged": "catalog:dev",
    "magic-string": "catalog:dev",
    "simple-git-hooks": "catalog:dev",
    "tinyexec": "catalog:utils",
    "tinyglobby": "catalog:dev",
    "tsdown": "catalog:dev",
    "tsx": "catalog:dev",
    "typescript": "catalog:dev",
    "unocss": "catalog:dev",
    "unplugin-vue": "catalog:dev",
    "vite": "catalog:dev",
    "vitest": "catalog:testing",
    "vitest-package-exports": "catalog:testing",
    "vue": "catalog:prod",
    "yaml": "catalog:testing"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
